{
  "name": "lynxprompt",
  "version": "1.2.0",
  "private": true,
  "description": "LynxPrompt - Transform your development setup into a mouse-click experience with AI IDE configuration generation",
  "author": "LynxPrompt Contributors",
  "license": "SEE LICENSE FILE",
  "repository": {
    "type": "git",
    "url": "https://github.com/GeiserX/LynxPrompt.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate --config=prisma/prisma.config-app.ts && prisma generate --config=prisma/prisma.config-users.ts && prisma generate --config=prisma/prisma.config-blog.ts && prisma generate --config=prisma/prisma.config-support.ts",
    "db:push": "prisma db push --config=prisma/prisma.config-app.ts && prisma db push --config=prisma/prisma.config-users.ts && prisma db push --config=prisma/prisma.config-blog.ts && prisma db push --config=prisma/prisma.config-support.ts",
    "db:push:app": "prisma db push --config=prisma/prisma.config-app.ts",
    "db:push:users": "prisma db push --config=prisma/prisma.config-users.ts",
    "db:push:blog": "prisma db push --config=prisma/prisma.config-blog.ts",
    "db:push:support": "prisma db push --config=prisma/prisma.config-support.ts",
    "db:seed": "tsx prisma/seed-app.ts",
    "db:seed:support": "tsx prisma/seed-support.ts",
    "db:seed:personas": "tsx prisma/local/seed-personas.ts",
    "db:studio:app": "prisma studio --config=prisma/prisma.config-app.ts",
    "db:studio:users": "prisma studio --config=prisma/prisma.config-users.ts",
    "db:studio:support": "prisma studio --config=prisma/prisma.config-support.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@sentry/nextjs": "^9.31.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@hookform/resolvers": "^3.9.1",
    "@marsidev/react-turnstile": "^1.4.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@tanstack/react-query": "^5.90.14",
    "@types/pg": "^8.16.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.4",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.7",
    "stripe": "^20.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@prisma/client": "^7.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "preact": "^10.28.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
